{
    "version": 2,
    "builds": [
        {
            "src": "webhook.py",
            "use": "@vercel/python",
            "config": {
                "runtime": "python3.9" 
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "webhook.py"
        }
    ]
}